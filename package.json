  "description": "MSME Udyam Registration Management System",
  "scripts": {
    "install:all": "cd backend && npm install && cd ../frontend && npm install && cd ../scrapper && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev", 
    "scrape": "cd scrapper && npm start",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npx prisma generate",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run preview",
    "test": "cd backend && npm test",
    "deploy:frontend": "echo 'Deploy frontend from frontend/ directory to Vercel'",
    "deploy:backend": "echo 'Deploy backend from backend/ directory to Render'"
  },
